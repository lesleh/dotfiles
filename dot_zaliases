if [[ "$OSTYPE" == "darwin"* ]]; then
  alias proxy-off='networksetup -setsocksfirewallproxystate "Wi-Fi" off'
  alias proxy-on='networksetup -setsocksfirewallproxystate "Wi-Fi" on'

  alias c="tr -d '\n' | pbcopy"
fi

alias be='bundle exec'
alias yw='yarn workspace'
alias yt='yarn test'
alias ytw='yarn test --watch'
alias gcne='git commit --no-edit'
